{
  "name": "@justinc/no-dups-validator",
  "version": "0.0.0-development",
  "description": "Checks for duplicates given an Iterable",
  "main": "lib/index.js",
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prereadme": "rimraf README.md",
    "readme": "jsdoc2md --template README.hbs ./lib/index.js >> README.md",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "no",
    "duplicates",
    "validator",
    "validation"
  ],
  "files": [
    "lib",
    "README.md"
  ],
  "author": "Justin Calleja <calleja.justin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "0.18.2",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "husky": "0.13.1",
    "jsdoc-to-markdown": "3.0.0",
    "rimraf": "2.6.0",
    "semantic-release": "6.3.2",
    "standard": "8.6.0",
    "validate-commit-msg": "^2.11.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justin-calleja/no-dups-validator.git"
  },
  "dependencies": {
    "@justinc/extract-duplicates": "1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
